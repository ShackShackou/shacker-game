# Shacker Game NFT Configuration
# Copy this file to .env and fill in your values

# Your NFT Contract Address (REQUIRED)
NFT_CONTRACT_ADDRESS=0x... # Replace with your actual NFT contract

# Ethereum RPC URL (optional - uses public by default)
ETH_RPC_URL=https://eth-mainnet.public.blastapi.io

# Other existing env vars
SUPABASE_SERVICE_KEY=your_supabase_key
JSONBIN_API_KEY=your_jsonbin_key
JWT_SECRET=your_jwt_secret

# Development mode (set to 'production' when live)
NODE_ENV=development